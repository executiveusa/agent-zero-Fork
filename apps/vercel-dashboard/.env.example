# Agent Zero Backend Configuration
AZ_BASE_URL=http://localhost:50001
AZ_API_KEY=your_mcp_server_token_here

# Optional: For backend session auth (if not using API key mode)
AZ_AUTH_LOGIN=
AZ_AUTH_PASSWORD=

# Dashboard Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=generate_secret_with_openssl_rand_base64_32

# Optional: Email allowlist (comma-separated)
DASH_ALLOWED_EMAILS=

# Optional: Redis for session storage (recommended for production)
UPSTASH_REDIS_URL=
UPSTASH_REDIS_TOKEN=

# Node Environment
NODE_ENV=development
